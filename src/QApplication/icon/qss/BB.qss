QDialog QPushButton{
    background-color: rgb(40, 47, 63);
    border: none;
    color: white;
}

QDialog QPushButton:hover{
    background-color: rgb(69, 187, 217);
    color: white;
}

QDialog QPushButton:pressed{
    background-color: rgb(63, 147, 168);
    color: white;
}


QDialog QLineEdit {
    padding: 1px;
    border-style: solid;
    border: 2px solid lightgray;
    border-radius: 8px;
    width:40px;
    height: 30px;
}


QDialog QToolButton{
	background-color: transparent;
    color: white;
}


QDialog QToolButton:hover{
    background-color: rgb(69, 187, 217);
    border: 1px;
}

QDialog QToolButton:disabled{
    background-color: gray;
    border: 1px;
}

QDialog QToolButton[popupMode="1"] { /* only for MenuButtonPopup */
    padding-right: 0px; /* make way for the popup button */
}

QDialog QToolButton::menu-button {
    border: none;
}
QToolButton::menu-arrow {
    image:url(:/skin/icon/icon/icons/light/appbar.chevron.down.png);
}
QToolButton::menu-indicator {
    image:url(:/skin/icon/icon/icons/light/appbar.chevron.down.png);
	subcontrol-position: right center;
	width: 16px;
    height: 16px;
}
QDialog QToolButton:open { /* when the button has its menu open */
    background-color: rgb(63, 147, 168);
}


QDialog QToolButton#close{
    background-color: transparent;
    color: white;
}


QDialog QToolButton#close:hover{
    background-color: red;
    border: 1px;
}

QDialog QToolButton#logo{
    background-color: transparent;
    color: white;
    border: 1px;
}


QDialog QToolButton#logo:hover{
    background-color: transparent;
    border: 1px;
}
QToolButton#User{
    border-left: 1px solid white;
    color: white;
}
QDialog QToolButton#User{
    border-left: 1px solid white;
    color: white;
}
QDialog QLabel#QFramer{
    height: 132;
    border-image: url(:/skin/images/logo.png);
}

QDialog QTextEdit#info{
    background-color: transparent;
    color:green;
    font-size: 16px;
    border: none;
}

QDialog QTabWidget::pane { /* The tab widget frame */
    border-top: 1px solid rgb(5, 9, 21);
}

QDialog QTabBar::tab {
    border-top: 1px solid black;
    border-right: 1px solid black;
    height: 40px;
    width: 80px;
	min-width: 80px;
	min-height: 40px;
    color: white;
    font-size: 15px;
    /*font-family: "Verdana";*/
    background-color: rgb(40, 47, 63);
}

QDialog QTabBar::tab:selected{
    background-color: rgb(63, 147, 168);
}

QDialog QTabBar::tab:hover {
    background-color: rgb(69, 187, 217);
}
QTabWidget::pane{
	border-top: 2px solid white;
	border-left:2px solid white;
	position: absolute;
}
QTabWidget::tab-bar {
	border-bottom: 2px solid white;
	border-left:2px solid white;
	alignment: left;
}
QTabBar::tab {
    border: none;
    border-bottom-color: white;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
	height: 40px;
    width: 80px;
	min-width: 80px;
	min-height: 40px;
    padding: 4px;
	color:white;
}
QTabBar::tab:hover {
	color:blue;
}
QTabBar::tab:selected {
	color:white;
	border-bottom: 6px solid blue;
	font-weight:bold;
}
QLabel#titleLabel{
	color:white;
}
QLabel#ContentLabel{
	color:white;
}
QAction#About{
  image: url(:/skin/icon/icon/icons/light/appbar.information.png);
}
FlowGraphicsViews{
    background-color: black;
}
CDockWidget{
    background-color: black;
}
CDockAreaWidget{
    background-color: black;
}
QFrame#CentralDock{
    background-color: black;
}
QtWidgetsBase#QFlowChart{
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(33, 50, 70, 255), stop:1 rgba(255, 255, 255, 255));
}