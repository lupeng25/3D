/***********************************************************/
/* QMainWindow Style*/
/***********************************************************/
QMainWindow{
    background-color: qradialgradient(spread:pad, cx:0.5, cy:0.5, radius:1, fx:0.5, fy:0.5, stop:0 rgba(119, 255, 255, 255), stop:1 rgba(32, 127, 155, 255));
}

QWigdet{
    background-color: transparent;
}

QToolBar {
    background: rgb(40, 47, 63);
    spacing: 0px; /* spacing between items in the tool bar */
}


QProgressBar {
    border: 1px solid white;
    text-align: right;
}

QProgressBar::chunk {
    background-color: lightgreen;
    width: 20px;
}

QProgressBar QLineEdit{
    border: none;
    color: black;
    padding-left: 5px;
    font-size: 16px;
    background-color: transparent;
}

/* ******************QMenu****************** */
QMenu {
    color: white;
    background-color: rgb(40, 47, 63);
    margin: 2px; 
}

QMenu::item {
    height: 23px;
    padding: 0px 25px 0px 20px;
    background: rgb(40, 47, 63);
}

QMenu::item:selected {
    background-color: rgb(69, 187, 217);
}

QMenu::icon:checked { /* appearance of a 'checked' icon */
    background: rgb(63, 147, 168);
    border: 1px inset gray;
    position: absolute;
    top: 1px;
    right: 1px;
    bottom: 1px;
    left: 1px;
 }

QMenu::item:disabled
{
    background-color: gray;
    border: 1px inset gray;
    position: absolute;
    top: 1px;
    right: 1px;
    bottom: 1px;
    left: 1px;
}


QMenu::separator {
    height: 2px;
    background: lightblue;
    margin-left: 10px;
    margin-right: 5px;
}

QMenu::indicator {
    width: 13px;
    height: 13px;
}
/* ******************QMenu****************** */

/* ******************QComboBox****************** */
QComboBox{
    color:white;
    font-size: 15px;
    font-family: "Verdana";
    background-color: rgb(5, 9, 21);
}

QComboBox:on{
    background-color: rgb(5, 9, 21);
}

QComboBox::drop-down :on{
    background-color: rgb(5, 9, 21);
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 15px;
}

QComboBox QAbstractItemView {
    border: none;
    color:white;
    background-color:rgb(47, 54, 65);
    font-size: 15px;
    font-family: "Verdana";
}


QComboBox QAbstractItemView::item{
    height: 40px;
    color:white;
    font-size: 15px;
}

QComboBox QAbstractItemView::item:selected{
    background-color: rgb(63, 147, 168);
}

QComboBox QAbstractItemView::item:hover{
    background-color: rgb(63, 147, 168);
}

QComboBox:disabled{
    background-color: gray;
}
QCheckBox{
    color:white;
}
QCheckBox::indicator:unchecked {
    image: url(:/image/icon/images/off.png);
}
QCheckBox::indicator:checked {
    image: url(:/image/icon/images/on.png);
}
/* ******************QComboBox****************** */
